<?xml version="1.0" encoding="UTF-8"?>
<tns:org xmlns:tns="http://www.ooi.org/org_spec" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.ooi.org/org_spec org_spec.xsd ">
  <tns:org_name>tns:org_name</tns:org_name>
  <tns:representative>tns:representative</tns:representative>
  <tns:role>
    <tns:name>org</tns:name>
    <tns:power>
      <tns:name>admit</tns:name>
      <tns:action>
        <tns:name>ADMIT</tns:name>
        <tns:param>
          <tns:name>org</tns:name>
          <tns:string_value>OOI</tns:string_value>
        </tns:param>
        <tns:param>
          <tns:name>member</tns:name>
          <tns:string_value id = "mem_id">var_1</tns:string_value>
        </tns:param>
        <tns:param>
          <tns:name>role</tns:name>
          <tns:string_value>var_3</tns:string_value>
        </tns:param>      
      	<tns:param>
          <tns:name>type</tns:name>
          <tns:string_value>membership</tns:string_value>
        </tns:param>
      </tns:action>
      <tns:authorization>
      <tns:authorization_id>admit_auth</tns:authorization_id>
      <tns:action>
        <tns:name>admit_req</tns:name>
        <tns:param>
          <tns:name>action</tns:name>
          <tns:string_value>REQUEST</tns:string_value>
        </tns:param>
        <tns:param>
          <tns:name>sender</tns:name>
          <tns:string_value idref = "mem_id">var_1</tns:string_value>
        </tns:param>
        <tns:param>
          <tns:name>receiver</tns:name>
          <tns:string_value>var_2</tns:string_value>
        </tns:param>      
      	<tns:param>
          <tns:name>type</tns:name>
          <tns:string_value>membership</tns:string_value>
        </tns:param>
      </tns:action> 
    </tns:authorization>
    </tns:power>
    <tns:power>
      <tns:name>eject</tns:name>
      <tns:action>
        <tns:name>EJECT</tns:name>
        <tns:param>
          <tns:name>org</tns:name>
          <tns:string_value>OOI</tns:string_value>
        </tns:param>
        <tns:param>
          <tns:name>member</tns:name>
          <tns:string_value>member_id</tns:string_value>
        </tns:param>
      </tns:action>
      
    </tns:power>
    <tns:capability>
      <tns:name>evaluate</tns:name>
      <tns:action>
        <tns:name>EVALUATE</tns:name>
        <tns:param>
          <tns:name>member</tns:name>
          <tns:string_value>member_id</tns:string_value>
        </tns:param>
        <tns:param>
          <tns:name>role</tns:name>
          <tns:string_value>role_id</tns:string_value>
        </tns:param>
      </tns:action>
    </tns:capability>
    <tns:commitment>
      <tns:debtor>org</tns:debtor>
      <tns:creditor>member</tns:creditor>
      <tns:antecedent>
        <tns:name>eval_request</tns:name>
        <tns:param>
          <tns:name>attribute_name</tns:name>
          <tns:string_value>value</tns:string_value>
        </tns:param>
      </tns:antecedent>
      <tns:consequent>
        <tns:name>eval_response</tns:name>
        <tns:param>
          <tns:name>attribute_name</tns:name>
          <tns:boolean_value>true</tns:boolean_value>
        </tns:param>
      </tns:consequent>
    </tns:commitment>
    <tns:prohibition>
      <tns:prohibition_id>prohibit_share</tns:prohibition_id>
      <tns:action>
        <tns:name>SHARE</tns:name>
        <tns:param>
          <tns:name>object</tns:name>
          <tns:string_value>obj_id</tns:string_value>
        </tns:param>
      </tns:action>
      <tns:sanction>
        <tns:name>EJECT</tns:name>
        <tns:param>
          <tns:name>org</tns:name>
          <tns:string_value>OOI</tns:string_value>
        </tns:param>
        <tns:param>
          <tns:name>member</tns:name>
          <tns:string_value>mem_id</tns:string_value>
        </tns:param>
      </tns:sanction>
    </tns:prohibition>
  </tns:role>
  <tns:role>
    <tns:name>member</tns:name>
    <tns:power>
      <tns:name>propose</tns:name>
      <tns:action>
        <tns:name>PROPOSE</tns:name>
        <tns:param>
          <tns:name>org</tns:name>
          <tns:string_value>OOI</tns:string_value>
        </tns:param>
      </tns:action>
    </tns:power>
    <tns:power>
      <tns:name>accept</tns:name>
      <tns:action>
        <tns:name>ACCEPT</tns:name>
        <tns:param>
          <tns:name>org</tns:name>
          <tns:string_value>OOI</tns:string_value>
        </tns:param>
        <tns:param>
          <tns:name>member</tns:name>
          <tns:string_value>member_id</tns:string_value>
        </tns:param>
      </tns:action>
    </tns:power>
    <tns:power>
      <tns:name>reject</tns:name>
      <tns:action>
        <tns:name>REJECT</tns:name>
        <tns:param>
          <tns:name>org</tns:name>
          <tns:string_value>OOI</tns:string_value>
        </tns:param>
        <tns:param>
          <tns:name>member</tns:name>
          <tns:string_value>member_id</tns:string_value>
        </tns:param>
      </tns:action>
    </tns:power>
    <tns:capability>
      <tns:name>req_evaluate</tns:name>
      <tns:action>
        <tns:name>REQ_EVALUATE</tns:name>
        <tns:param>
          <tns:name>org</tns:name>
          <tns:string_value>org_id</tns:string_value>
        </tns:param>
        <tns:param>
          <tns:name>attribute_name</tns:name>
          <tns:string_value>role</tns:string_value>
        </tns:param>
        <tns:param>
          <tns:name>attribute_value</tns:name>
          <tns:string_value>member</tns:string_value>
        </tns:param>
      </tns:action>
    </tns:capability>
    <tns:commitment>
      <tns:debtor>org</tns:debtor>
      <tns:creditor>member</tns:creditor>
      <tns:antecedent>
        <tns:name>eval_request</tns:name>
        <tns:param>
          <tns:name>attribute_name</tns:name>
          <tns:string_value>value</tns:string_value>
        </tns:param>
      </tns:antecedent>
      <tns:consequent>
        <tns:name>eval_response</tns:name>
        <tns:param>
          <tns:name>attribute_name</tns:name>
          <tns:boolean_value>true</tns:boolean_value>
        </tns:param>
      </tns:consequent>
    </tns:commitment>
    <tns:prohibition>
      <tns:prohibition_id>prohibit_share</tns:prohibition_id>
      <tns:action>
        <tns:name>SHARE</tns:name>
        <tns:param>
          <tns:name>object</tns:name>
          <tns:string_value>obj_id</tns:string_value>
        </tns:param>
      </tns:action>
      <tns:sanction>
        <tns:name>EJECT</tns:name>
        <tns:param>
          <tns:name>org</tns:name>
          <tns:string_value>OOI</tns:string_value>
        </tns:param>
        <tns:param>
          <tns:name>member</tns:name>
          <tns:string_value>mem_id</tns:string_value>
        </tns:param>
      </tns:sanction>
    </tns:prohibition>	
  </tns:role>
</tns:org>
